{
    "compilerOptions": {
      "baseUrl": ".",
      "inlineSourceMap": true,
      "inlineSources": true,
      "target": "ES2020",
      "module": "ESNext",
      "lib": ["ES2020", "DOM"],
      "moduleResolution": "Bundler",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "types": ["obsidian", "jest", "node"],
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts", "*.ts", "tests/**/*.ts"]
  }
  